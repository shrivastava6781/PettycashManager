<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8" />
  <link rel="shortcut icon" href="favicon.ico" type="image/x-icon">
  <link rel="shortcut icon" href=`${process.env.REACT_APP_LOCAL_URL}/uploads/settings/${favicon}` type="image/x-icon">
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <meta name="theme-color" content="#000000" />
  <meta
    name="description"
    content="Web site created using create-react-app"
  />
  <title>Asset Management</title>

  <!-- Bootstrap core CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
  
  <!-- Custom fonts for this template-->
  <link href="vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
  <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

  <!-- Custom styles for this template-->
  <link href="css/sb-admin-2.min.css" rel="stylesheet">

  <!-- Script to fetch favicon from backend -->
  <script>
    window.addEventListener('DOMContentLoaded', async () => {
      try {
        const response = await fetch(`http://localhost:5000/settings`);
        const data = await response.json();

  
        if (data && data.favicon) {
          // Set favicon
          const favicon = document.querySelector('link[rel="icon"]');
          favicon.href = data.favicon;
  
          // Log success message
          console.log('Favicon successfully fetched and set.');
        } else {
          console.log('Favicon data not found in the response.');
        }
      } catch (error) {
        console.error('Error fetching favicon:', error);
      }
    });
  </script>
  
</head>
<body>
  <noscript>You need to enable JavaScript to run this app.</noscript>
  <div id="root"></div>
  
